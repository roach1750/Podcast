source 'https://github.com/CocoaPods/Specs.git'


use_frameworks!




target ‘Podcast’ do
  	platform :ios, '12.0'
inhibit_all_warnings!

	pod 'RealmSwift'
    	pod 'Alamofire', '~> 4.7'
    	pod 'Kanna', '~> 4.0.0'
  	pod 'FeedKit', '~> 6.0'
	pod 'NVActivityIndicatorView'

end

target 'WatchPodcast Extension' do
    platform :watchos, '5.0'
	pod 'RealmSwift'

end


