source 'https://github.com/CocoaPods/Specs.git'


use_frameworks!




target ‘Podcast’ do
  	platform :ios, '10.0'
	pod 'RealmSwift'
    	pod 'Alamofire', '~> 4.5'
    	pod 'Kanna', '~> 2.1.0'
  	pod 'FeedKit', '~> 6.0'
pod 'NVActivityIndicatorView'
end

target 'Watch Podcast Extension' do
    platform :watchos, '3.0'
	pod 'RealmSwift'

end


